{
  "name": "vuex-persist-indexeddb",
  "description": "Persist and rehydrate your Vuex state between page reloads.",
  "version": "0.1.2",
  "license": "MIT",
  "author": "sunhuaixing <sunhuaixing@gmail.com> ",
  "keywords": [
    "vue",
    "vuex",
    "plugin"
  ],
  "homepage": "https://github.com/kaodigua/vuex-persist-indexeddb#readme",
  "repository": "kaodigua/vuex-persist-indexeddb",
  "bugs": {
    "url": "https://github.com/kaodigua/vuex-persist-indexeddb/issues"
  },
  "source": "src/index.js",
  "main": "dist/vuex-persist-indexeddb.js",
  "module": "dist/vuex-persist-indexeddb.es.js",
  "unpkg": "dist/vuex-persist-indexeddb.umd.js",
  "types": "dist/index.d.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rimraf dist && microbundle --external all --name createPersist",
    "prepare": "npm run build",
    "test": "npm-run-all test:**",
    "test:jest": "jest",
    "test:size": "bundlesize"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": false
        }
      ]
    ],
    "plugins": [
      ["@babel/plugin-transform-runtime",
        {
          "absoluteRuntime": false,
          "corejs": false,
          "helpers": false,
          "regenerator": true
        }]
    ]
  },
  "bundlesize": [
    {
      "path": "./dist/*.js",
      "threshold": "800b"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && pretty-quick --staged"
    }
  },
  "jest": {
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "localforage": "^1.9.0",
    "lodash": "^4.17.21",
    "shvl": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime": "^7.14.6",
    "all-contributors-cli": "^6.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.0.2",
    "bundlesize": "^0.18.1",
    "dom-storage": "^2.0.2",
    "eslint": "^7.17.0",
    "husky": "^5.1.2",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.0",
    "vue": "^2.5.15",
    "vuex": "^3.0.1"
  },
  "peerDependencies": {
    "vue": "^2.0.0",
    "vuex": "^2.0.0 || ^3.0.0"
  }
}
